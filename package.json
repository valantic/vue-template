{
  "name": "vue-template",
  "version": "7.0.0",
  "description": "A customized Vue.js project template",
  "lastDependencyUpdate": "01.02.2021",
  "scripts": {
    "clean:caches": "rm -rf .eslintcache .stylelintcache node_modules/.cache",
    "dev": "webpack-dev-server --mode development",
    "dev:s": "npm run styleguide",
    "jest": "jest tests --config jest.config.js --silent",
    "styleguide": "vue-styleguidist server",
    "test": "eslint --cache --ext .js,.vue src tests/unit/specs && stylelint --cache 'src/**/*.?(vue|scss)' && npm run jest; exit 0;",
    "build": "NODE_ENV=production webpack --mode production --progress",
    "build:profile": "NODE_ENV=production webpack --mode production --profile",
    "build:styleguide": "webpack --styleguide-build && vue-styleguidist build",
    "build:watch": "NODE_ENV=production webpack --mode production --watch"
  },
  "webpack": {
    "buildPath": "dist",
    "productionPath": "/",
    "styleguidePath": "/",
    "developmentPath": "/",
    "outputAssetsFolder": "assets/",
    "filePrefix": "",
    "themeSource": "src/setup/scss/themes/",
    "themeFiles": [
      "theme-01",
      "theme-02"
    ],
    "devPort": 8080
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run jest",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint"
    ],
    "*.{css,vue,scss}": [
      "stylelint"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/valantic/vue-template.git"
  },
  "engines": {
    "node": "12.14",
    "npm": ">=6.13.1"
  },
  "keywords": [
    "vue",
    "vuejs",
    "valantic"
  ],
  "author": "valantic CEC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/valantic/vue-template/issues"
  },
  "homepage": "https://github.com/valantic/vue-template",
  "dependencies": {
    "axios": "~0.20.0",
    "core-js": "~3.6.5",
    "css-vars-ponyfill": "~2.3.2",
    "dayjs": "~1.9.1",
    "picturefill": "~3.0.3",
    "portal-vue": "~2.1.7",
    "swiper": "~6.5.1",
    "vue": "~2.6.12",
    "vue-i18n": "~8.21.1",
    "vue-js-modal": "~1.3.35",
    "vuex": "~3.5.1"
  },
  "devDependencies": {
    "@babel/core": "~7.11.6",
    "@babel/plugin-syntax-dynamic-import": "~7.8.3",
    "@babel/preset-env": "~7.11.5",
    "@vue/test-utils": "~1.1.0",
    "autoprefixer": "~10.0.1",
    "axios-mock-adapter": "~1.18.2",
    "babel-core": "~7.0.0-bridge.0",
    "babel-eslint": "~10.0.3",
    "babel-jest": "~26.3.0",
    "babel-loader": "~8.1.0",
    "chalk": "~4.1.2",
    "clean-webpack-plugin": "~3.0.0",
    "css-loader": "~4.3.0",
    "cssnano": "~4.1.10",
    "eslint": "~7.10.0",
    "eslint-config-airbnb-base": "~14.2.0",
    "eslint-config-valantic": "~8.0.0",
    "eslint-import-resolver-webpack": "~0.13.0",
    "eslint-plugin-import": "~2.22.1",
    "eslint-plugin-vue": "~6.2.2",
    "eslint-webpack-plugin": "~2.1.0",
    "faker": "~5.1.0",
    "file-loader": "~6.1.0",
    "friendly-errors-webpack-plugin": "~1.7.0",
    "html-webpack-plugin": "~4.5.0",
    "husky": "~4.3.0",
    "image-webpack-loader": "~7.0.1",
    "jest": "~26.4.2",
    "jest-transform-stub": "~2.0.0",
    "launch-editor-middleware": "~2.2.1",
    "lint-staged": "~10.4.0",
    "mini-css-extract-plugin": "~0.11.2",
    "postcss": "~8.1.1",
    "postcss-loader": "~4.0.2",
    "sass": "~1.26.11",
    "sass-loader": "~10.0.2",
    "sass-resources-loader": "~2.1.1",
    "stylelint": "~13.7.2",
    "stylelint-config-valantic": "~6.0.0",
    "stylelint-webpack-plugin": "~2.1.0",
    "stylus": "~0.54.8",
    "stylus-loader": "~3.0.2",
    "uglifyjs-webpack-plugin": "~2.2.0",
    "vue-jest": "~3.0.7",
    "vue-loader": "~15.9.3",
    "vue-markdown-loader": "~2.4.1",
    "vue-router": "~3.4.5",
    "vue-styleguidist": "~4.32.4",
    "vue-template-compiler": "~2.6.12",
    "webpack": "~4.44.2",
    "webpack-bundle-analyzer": "~3.9.0",
    "webpack-cli": "~3.3.12",
    "webpack-dependency-hint": "git+https://github.com/valantic/webpack-dependency-hint.git",
    "webpack-dev-server": "~3.11.0",
    "webpack-manifest-plugin": "~2.2.0"
  },
  "browserslist": [
    "> 2%",
    "last 2 years",
    "ie 11",
    "not ie_mob > 0",
    "not dead"
  ]
}
