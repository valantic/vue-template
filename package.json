{
  "name": "vue-template",
  "version": "8.0.0",
  "description": "A customized Vue.js project template",
  "type": "module",
  "lastDependencyUpdate": "15.02.2023",
  "scripts": {
    "clean:caches": "rm -rf .eslintcache .stylelintcache node_modules/.cache",
    "dev": "vite",
    "lint": "npm run lint:eslint && npm run lint:stylelint",
    "lint:eslint": "eslint --ext .ts,.js,.vue ./",
    "lint:stylelint": "stylelint 'src/**/*.?(vue|scss)'",
    "lint-staged": "lint-staged",
    "test": "npm run tsc && npm run lint && npm run test:unit -- --watch=false",
    "test:unit": "vitest --dir tests",
    "build": "vite build --mode=app",
    "build:icons": "svg-sprite --config .svg-sprite.json src/assets/icons/*.svg; node ./update-ts-icon-type",
    "build:profile": "npm run build -- --mode=profile",
    "build:storybook": "storybook build --output-dir dist/storybook",
    "build:watch": "npm run build -- --watch",
    "serve": "vite preview",
    "prepare": "husky",
    "prettier": "prettier . --write",
    "storybook": "storybook dev -p 6006",
    "tsc": "vue-tsc"
  },
  "webpack": {
    "buildPath": "dist",
    "localDist": "webpack-dist",
    "productionPath": "/",
    "styleguideBuildPath": "./dist",
    "styleguidePath": "/",
    "developmentPath": "/",
    "outputAssetsFolder": "assets",
    "appName": "app",
    "filePrefix": "",
    "themeSource": "src/setup/scss/themes/",
    "themeFiles": [
      "theme-01",
      "theme-02"
    ],
    "devPort": 9090
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/valantic/vue-template.git"
  },
  "engines": {
    "node": ">=20.10 <21",
    "npm": ">=10 <11"
  },
  "keywords": [
    "vue",
    "vuejs",
    "valantic"
  ],
  "author": "valantic CEC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/valantic/vue-template/issues"
  },
  "homepage": "https://github.com/valantic/vue-template",
  "dependencies": {
    "@popperjs/core": "~2.11.8",
    "@vuelidate/core": "~2.0.3",
    "@vuelidate/validators": "~2.0.4",
    "axios": "~1.6.8",
    "body-scroll-lock": "~4.0.0-beta.0",
    "dayjs": "~1.11.11",
    "embla-carousel": "~7.0.9",
    "embla-carousel-autoplay": "~7.0.9",
    "pikaday": "~1.8.2",
    "pinia": "~2.1.7",
    "the-new-css-reset": "~1.11.2",
    "vite": "~5.2.11",
    "vue": "~3.4.27",
    "vue-i18n": "~9.13.1"
  },
  "devDependencies": {
    "@faker-js/faker": "~8.4.1",
    "@storybook/addon-actions": "~7.6.8",
    "@storybook/addon-essentials": "~7.6.8",
    "@storybook/addon-interactions": "~7.6.8",
    "@storybook/addon-links": "~7.6.8",
    "@storybook/preset-scss": "~1.0.3",
    "@storybook/testing-library": "~0.2.2",
    "@storybook/vue3": "~7.6.8",
    "@storybook/vue3-vite": "~7.6.8",
    "@trivago/prettier-plugin-sort-imports": "~4.3.0",
    "@types/body-scroll-lock": "~3.1.2",
    "@types/cookiebot-sdk": "~2.43.5",
    "@types/google.maps": "~3.55.8",
    "@types/node": "~20.12.11",
    "@types/pikaday": "~1.7.6",
    "@types/resize-observer-browser": "~0.1.11",
    "@typescript-eslint/eslint-plugin": "~7.8.0",
    "@typescript-eslint/parser": "~7.8.0",
    "@vitejs/plugin-vue": "~5.0.4",
    "@vue/eslint-config-typescript": "~13.0.0",
    "@vue/test-utils": "~2.4.3",
    "autoprefixer": "~10.4.19",
    "cssnano": "~7.0.1",
    "eslint": "~8.56.0",
    "eslint-config-airbnb-base": "~15.0.0",
    "eslint-config-prettier": "~9.1.0",
    "eslint-config-valantic": "~16.0.1",
    "eslint-import-resolver-typescript": "~3.6.1",
    "eslint-import-resolver-vite": "~1.1.0",
    "eslint-plugin-import": "~2.29.1",
    "eslint-plugin-storybook": "~0.6.15",
    "eslint-plugin-vue": "~9.25.0",
    "husky": "~9.0.11",
    "jsdom": "~24.0.0",
    "lint-staged": "~15.2.2",
    "msw": "~2.3.0",
    "postcss": "~8.4.33",
    "postcss-html": "~1.7.0",
    "postcss-scss": "~4.0.9",
    "react": "~18.3.1",
    "react-dom": "~18.3.1",
    "rollup-plugin-visualizer": "~5.12.0",
    "sass": "~1.77.1",
    "storybook": "~7.6.8",
    "stylelint": "~16.5.0",
    "stylelint-config-valantic": "~9.0.0",
    "svg-sprite": "~2.0.2",
    "svgo": "~3.3.2",
    "typescript": "~5.4.5",
    "vite-plugin-image-optimizer": "~1.1.7",
    "vite-plugin-markdown": "~2.2.0",
    "vitest": "~1.6.0",
    "vue-component-type-helpers": "~2.0.17",
    "vue-router": "~4.3.2",
    "vue-tsc": "~2.0.17"
  },
  "browserslist": [
    "> 2%",
    "last 2 years",
    "not ie_mob > 0",
    "not ie > 0",
    "not dead"
  ],
  "msw": {
    "workerDirectory": "public"
  }
}
