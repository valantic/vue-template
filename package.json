{
  "name": "vue-template",
  "version": "4.0.0-alpha",
  "description": "A customized Vue.js project template",
  "scripts": {
    "clean:caches": "rm -f .eslintcache .stylelintcache node_modules/.cache",
    "dev": "webpack-dev-server --mode development",
    "dev:s": "npm run styleguide",
    "jest": "jest tests --config jest.config.js",
    "styleguide": "vue-styleguidist server",
    "test": "eslint --cache --ext .js,.vue src tests/unit/specs && stylelint --cache 'src/**/*.?(vue|scss)' && npm run jest; exit 0;",
    "build": "webpack --mode production --progress",
    "build:profile": "webpack --mode production --profile",
    "build:styleguide": "webpack --mode production --styleguide",
    "build:watch": "webpack --mode production --watch"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run jest",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint"
    ],
    "*.{css,vue,scss}": [
      "stylelint"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/valantic/vue-template.git"
  },
  "engines": {
    "node": "12.14",
    "npm": ">=6.13.1"
  },
  "keywords": [
    "vue",
    "vuejs",
    "valantic"
  ],
  "author": "valantic CEC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/valantic/vue-template/issues"
  },
  "homepage": "https://github.com/valantic/vue-template",
  "dependencies": {
    "@babel/polyfill": "~7.4.4",
    "axios": "~0.19.0",
    "core-js": "~3.1.3",
    "css-vars-ponyfill": "~2.0.2",
    "dayjs": "~1.8.14",
    "lazysizes": "~5.1.0",
    "material-design-icons-iconfont": "~5.0.1",
    "picturefill": "~3.0.3",
    "portal-vue": "~2.1.7",
    "swiper": "~4.5.0",
    "vue": "~2.6.11",
    "vue-i18n": "~8.15.3",
    "vue-js-modal": "~1.3.31",
    "vue-router": "~3.1.5",
    "vue-tabs-component": "~1.5.0",
    "vuetify": "~1.5.14",
    "vuex": "~3.1.2"
  },
  "devDependencies": {
    "@babel/core": "~7.4.5",
    "@babel/plugin-syntax-dynamic-import": "~7.2.0",
    "@babel/preset-env": "~7.4.5",
    "@vue/test-utils": "~1.0.0-beta.29",
    "autoprefixer": "~9.7.4",
    "axios-mock-adapter": "~1.16.0",
    "babel-core": "~7.0.0-bridge.0",
    "babel-eslint": "~10.0.1",
    "babel-jest": "~24.8.0",
    "babel-loader": "~8.0.6",
    "clean-webpack-plugin": "~3.0.0",
    "css-loader": "~2.1.1",
    "cssnano": "~4.1.10",
    "eslint": "~5.16.0",
    "eslint-config-airbnb-base": "~13.1.0",
    "eslint-config-valantic": "~6.2.0",
    "eslint-import-resolver-webpack": "~0.11.1",
    "eslint-loader": "~2.1.2",
    "eslint-plugin-import": "~2.17.3",
    "eslint-plugin-vue": "~5.2.2",
    "faker": "~4.1.0",
    "file-loader": "~4.0.0",
    "friendly-errors-webpack-plugin": "~1.7.0",
    "html-webpack-plugin": "~3.2.0",
    "husky": "~2.4.1",
    "image-webpack-loader": "~5.0.0",
    "jest": "~24.8.0",
    "jest-transform-stub": "~2.0.0",
    "launch-editor-middleware": "~2.2.1",
    "lint-staged": "~8.2.1",
    "mini-css-extract-plugin": "~0.9.0",
    "node-sass": "~4.13.1",
    "postcss-loader": "~3.0.0",
    "sass-loader": "~7.1.0",
    "sass-resources-loader": "~2.0.1",
    "stylelint": "~10.1.0",
    "stylelint-config-valantic": "~4.0.1",
    "stylelint-webpack-plugin": "~0.10.5",
    "stylus": "~0.54.7",
    "stylus-loader": "~3.0.2",
    "uglifyjs-webpack-plugin": "~2.2.0",
    "url-loader": "~2.0.0",
    "vue-jest": "~3.0.5",
    "vue-loader": "~15.8.3",
    "vue-markdown-loader": "~2.4.1",
    "vue-styleguidist": "~3.14.2",
    "vue-template-compiler": "~2.6.11",
    "webpack": "~4.41.5",
    "webpack-bundle-analyzer": "~3.6.0",
    "webpack-clean": "~1.2.3",
    "webpack-cli": "~3.3.10",
    "webpack-dev-server": "~3.10.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
