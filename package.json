{
  "name": "vue-template",
  "version": "8.0.0",
  "description": "A customized Vue.js project template",
  "lastDependencyUpdate": "15.02.2023",
  "scripts": {
    "clean:caches": "rm -rf .eslintcache .stylelintcache node_modules/.cache",
    "dev": "vite",
    "fix:eslint": "npm run lint:eslint -- --config .eslintrc.fix.js --cache=false --fix",
    "fix:stylelint": "npm run lint:stylelint -- --cache=false --config .stylelintrc.fix.js --fix",
    "lint": "npm run lint:eslint && npm run lint:stylelint",
    "lint:eslint": "eslint --ext .ts,.js,.vue ./",
    "lint:stylelint": "stylelint 'src/**/*.?(vue|scss)'",
    "lint-staged": "lint-staged",
    "test": "npm run tsc && npm run lint && npm run test:unit -- --watch=false",
    "test:unit": "vitest --dir tests",
    "build": "vite build --mode=app",
    "build:icons": "svg-sprite --config .svg-sprite.json src/assets/icons/*.svg",
    "build:profile": "npm run build -- --mode=profile",
    "build:storybook": "build-storybook --output-dir dist/storybook",
    "build:watch": "npm run build -- --watch",
    "serve": "vite preview",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "tsc": "vue-tsc --noEmit"
  },
  "webpack": {
    "buildPath": "dist",
    "localDist": "webpack-dist",
    "productionPath": "/",
    "styleguideBuildPath": "./dist",
    "styleguidePath": "/",
    "developmentPath": "/",
    "outputAssetsFolder": "assets",
    "appName": "app",
    "filePrefix": "",
    "themeSource": "src/setup/scss/themes/",
    "themeFiles": [
      "theme-01",
      "theme-02"
    ],
    "devPort": 9090
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/valantic/vue-template.git"
  },
  "engines": {
    "node": ">=18 <19",
    "npm": ">=8.19"
  },
  "keywords": [
    "vue",
    "vuejs",
    "valantic"
  ],
  "author": "valantic CEC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/valantic/vue-template/issues"
  },
  "homepage": "https://github.com/valantic/vue-template",
  "dependencies": {
    "@popperjs/core": "~2.11.8",
    "@vuelidate/core": "~2.0.3",
    "@vuelidate/validators": "~2.0.4",
    "axios": "~1.5.0",
    "body-scroll-lock": "~4.0.0-beta.0",
    "dayjs": "~1.11.9",
    "embla-carousel": "~7.0.9",
    "embla-carousel-autoplay": "~7.0.9",
    "pinia": "~2.1.6",
    "the-new-css-reset": "~1.9.0",
    "vue": "~3.3.4",
    "vue-i18n": "~9.3.0"
  },
  "devDependencies": {
    "@faker-js/faker": "~8.0.2",
    "@storybook/addon-actions": "~6.5.16",
    "@storybook/addon-essentials": "~6.5.16",
    "@storybook/addon-interactions": "~6.5.16",
    "@storybook/addon-links": "~6.5.16",
    "@storybook/builder-vite": "~0.4.2",
    "@storybook/preset-scss": "~1.0.3",
    "@storybook/testing-library": "^0.0.13",
    "@storybook/vue3": "~6.5.16",
    "@types/body-scroll-lock": "~3.1.0",
    "@types/google.maps": "~3.54.0",
    "@types/node": "~20.6.0",
    "@types/resize-observer-browser": "~0.1.6",
    "@typescript-eslint/eslint-plugin": "~5.52.0",
    "@typescript-eslint/parser": "~5.52.0",
    "@vitejs/plugin-vue": "~4.3.4",
    "@vue/eslint-config-typescript": "~11.0.3",
    "@vue/test-utils": "~2.4.1",
    "autoprefixer": "~10.4.15",
    "cssnano": "~6.0.1",
    "eslint": "~8.49.0",
    "eslint-config-airbnb-base": "~15.0.0",
    "eslint-config-valantic": "~16.0.1",
    "eslint-import-resolver-vite": "~1.3.0",
    "eslint-plugin-import": "~2.28.1",
    "eslint-plugin-storybook": "~0.6.13",
    "eslint-plugin-vue": "~9.17.0",
    "husky": "~8.0.3",
    "jsdom": "~22.1.0",
    "lint-staged": "~14.0.1",
    "msw": "~1.3.0",
    "postcss": "~8.4.18",
    "postcss-html": "~1.5.0",
    "postcss-scss": "~4.0.8",
    "rollup-plugin-visualizer": "~5.9.2",
    "sass": "~1.66.1",
    "stylelint": "~15.10.3",
    "stylelint-config-valantic": "~8.0.1",
    "svg-sprite": "~2.0.2",
    "svgo": "~3.0.2",
    "typescript": "~4.9.5",
    "vite": "~4.4.9",
    "vite-plugin-image-optimizer": "~1.1.7",
    "vite-plugin-markdown": "~2.1.0",
    "vitest": "~0.34.4",
    "vue-router": "~4.2.4",
    "vue-tsc": "~1.8.10"
  },
  "msw": {
    "workerDirectory": "public"
  }
}
