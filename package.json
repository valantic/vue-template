{
  "name": "vue-template",
  "version": "7.0.0",
  "description": "A customized Vue.js project template",
  "scripts": {
    "clean:caches": "rm -rf .eslintcache .stylelintcache node_modules/.cache",
    "dev": "webpack-dev-server --mode development",
    "dev:s": "npm run styleguide",
    "jest": "jest tests --config jest.config.js --silent",
    "styleguide": "vue-styleguidist server",
    "test": "eslint --cache --ext .js,.vue src tests/unit/specs && stylelint --cache 'src/**/*.?(vue|scss)' && npm run jest; exit 0;",
    "build": "NODE_ENV=production webpack --mode production --progress",
    "build:profile": "NODE_ENV=production webpack --mode production --profile",
    "build:styleguide": "webpack --styleguide-build && vue-styleguidist build",
    "build:watch": "NODE_ENV=production webpack --mode production --watch",
    "build:icons": "svg-sprite --config .svg-sprite.json src/assets/icons/*.svg",
    "lint-staged": "lint-staged",
    "prepare": "husky install"
  },
  "webpack": {
    "buildPath": "dist",
    "localDist": "webpack-dist",
    "productionPath": "/",
    "styleguideBuildPath": "./dist",
    "styleguidePath": "/",
    "developmentPath": "/",
    "outputAssetsFolder": "assets/",
    "filePrefix": "",
    "themeSource": "src/setup/scss/themes/",
    "themeFiles": [
      "theme-01",
      "theme-02"
    ],
    "devPort": 9090
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint"
    ],
    "*.{css,vue,scss}": [
      "stylelint"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/valantic/vue-template.git"
  },
  "engines": {
    "node": ">=16 <17",
    "npm": ">=7.10"
  },
  "keywords": [
    "vue",
    "vuejs",
    "valantic"
  ],
  "author": "valantic CEC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/valantic/vue-template/issues"
  },
  "homepage": "https://github.com/valantic/vue-template",
  "dependencies": {
    "axios": "~0.21.1",
    "core-js": "~3.11.0",
    "css-vars-ponyfill": "~2.4.3",
    "dayjs": "~1.10.4",
    "picturefill": "~3.0.3",
    "portal-vue": "~2.1.7",
    "reset-css": "~5.0.1",
    "swiper": "~6.5.8",
    "vue": "~2.6.12",
    "vue-i18n": "~8.24.3",
    "vue-js-modal": "~1.3.35",
    "vuelidate": "~0.7.6",
    "vuex": "~3.6.2"
  },
  "devDependencies": {
    "@babel/core": "~7.13.16",
    "@babel/eslint-parser": "~7.13.14",
    "@babel/plugin-syntax-dynamic-import": "~7.8.3",
    "@babel/preset-env": "~7.13.15",
    "@vue/test-utils": "~1.1.4",
    "autoprefixer": "~10.2.5",
    "axios-mock-adapter": "~1.19.0",
    "babel-core": "~7.0.0-bridge.0",
    "babel-jest": "~26.6.3",
    "babel-loader": "~8.2.2",
    "clean-webpack-plugin": "~3.0.0",
    "css-loader": "~5.2.4",
    "cssnano": "~5.0.1",
    "eslint": "~7.25.0",
    "eslint-config-airbnb-base": "~14.2.1",
    "eslint-config-valantic": "~9.1.0",
    "eslint-import-resolver-webpack": "~0.13.2",
    "eslint-plugin-import": "~2.22.1",
    "eslint-plugin-vue": "~7.9.0",
    "eslint-webpack-plugin": "~2.5.4",
    "faker": "~5.5.3",
    "file-loader": "~6.2.0",
    "friendly-errors-webpack-plugin": "~1.7.0",
    "html-webpack-plugin": "~4.5.2",
    "husky": "~6.0.0",
    "image-webpack-loader": "~7.0.1",
    "jest": "~26.6.3",
    "jest-transform-stub": "~2.0.0",
    "launch-editor-middleware": "~2.2.1",
    "lint-staged": "~10.5.4",
    "mini-css-extract-plugin": "~1.5.0",
    "postcss-loader": "~4.2.0",
    "react": "~17.0.2",
    "react-dom": "~17.0.2",
    "sass": "~1.39.2",
    "sass-loader": "~10.1.1",
    "stylelint": "~13.13.0",
    "stylelint-config-valantic": "~6.4.0",
    "stylelint-webpack-plugin": "~2.1.1",
    "svg-sprite": "~1.5.2",
    "svg4everybody": "~2.1.9",
    "terser-webpack-plugin": "~4.2.3",
    "vue-jest": "~3.0.7",
    "vue-loader": "~15.9.6",
    "vue-markdown-loader": "~2.4.1",
    "vue-router": "~3.5.1",
    "vue-styleguidist": "~4.41.2",
    "vue-template-compiler": "~2.6.12",
    "webpack": "~4.44.2",
    "webpack-assets-manifest": "~3.1.1",
    "webpack-bundle-analyzer": "~3.9.0",
    "webpack-cli": "~3.3.12",
    "webpack-dev-server": "~3.11.2"
  },
  "browserslist": [
    "> 2%",
    "last 2 years",
    "ie 11",
    "not ie_mob > 0",
    "not dead"
  ]
}
