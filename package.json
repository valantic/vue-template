{
  "name": "vue-template",
  "version": "8.0.0",
  "description": "A customized Vue.js project template",
  "type": "module",
  "lastDependencyUpdate": "02.10.2025",
  "scripts": {
    "clean:caches": "rm -rf .eslintcache .stylelintcache node_modules/.cache",
    "dev": "vite",
    "fix:stylelint": "npm run lint:stylelint -- --cache=false --config .stylelintrc.fix.js --fix",
    "lint": "npm run lint:eslint && npm run lint:stylelint && npm run tsc",
    "lint:eslint": "eslint ./",
    "lint:stylelint": "stylelint 'src/**/*.?(vue|scss)'",
    "lint-staged": "lint-staged",
    "test": "npm run lint && npm run test:unit -- --watch=false",
    "test:unit": "vitest --dir tests",
    "build": "vite build --mode=app",
    "build:icons": "svg-sprite --config .svg-sprite.json src/assets/icons/*.svg; node ./update-ts-icon-type",
    "build:profile": "npm run build -- --mode=profile",
    "build:watch": "npm run build -- --watch",
    "serve": "vite preview",
    "prepare": "husky",
    "prettier": "prettier . --write",
    "tsc": "vue-tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/valantic/vue-template.git"
  },
  "engines": {
    "node": ">=22 <25",
    "npm": ">=10 <12"
  },
  "keywords": [
    "vue",
    "vuejs",
    "valantic"
  ],
  "author": "valantic CEC",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/valantic/vue-template/issues"
  },
  "homepage": "https://github.com/valantic/vue-template",
  "dependencies": {
    "@valantic/scss-utils": "github:valantic/scss-utils#feature/npm-update-sept-2025",
    "@vuelidate/core": "~2.0.3",
    "@vuelidate/validators": "~2.0.4",
    "axios": "~1.12.2",
    "body-scroll-lock": "~4.0.0-beta.0",
    "dayjs": "~1.11.18",
    "embla-carousel": "~8.6.0",
    "embla-carousel-autoplay": "~8.6.0",
    "floating-vue": "~5.2.2",
    "pikaday": "~1.8.2",
    "pinia": "~3.0.2",
    "the-new-css-reset": "~1.11.3",
    "vite": "~7.1.8",
    "vue": "~3.5.22",
    "vue-i18n": "~11.1.12"
  },
  "devDependencies": {
    "@eslint/eslintrc": "~3.3.1",
    "@eslint/js": "~9.36.0",
    "@faker-js/faker": "~10.0.0",
    "@trivago/prettier-plugin-sort-imports": "~5.2.2",
    "@types/body-scroll-lock": "~3.1.2",
    "@types/cookiebot-sdk": "~2.43.5",
    "@types/google.maps": "~3.58.1",
    "@types/node": "~24.6.2",
    "@types/pikaday": "~1.7.9",
    "@types/resize-observer-browser": "~0.1.11",
    "@valantic/vue-styleguide": "github:valantic/vue-styleguide#feature/npm-update",
    "@vitejs/plugin-vue": "~6.0.1",
    "@vue/test-utils": "~2.4.6",
    "autoprefixer": "~10.4.20",
    "cssnano": "~7.1.1",
    "eslint": "~9.36.0",
    "eslint-config-prettier": "~10.1.5",
    "eslint-config-valantic": "~18.0.0",
    "eslint-import-resolver-typescript": "~4.4.4",
    "eslint-import-resolver-vite": "~2.1.0",
    "eslint-plugin-import": "~2.32.0",
    "eslint-plugin-jsdoc": "~60.7.1",
    "eslint-plugin-unicorn": "~61.0.2",
    "eslint-plugin-vue": "~10.5.0",
    "globals": "~16.4.0",
    "husky": "~9.1.7",
    "jsdom": "~27.0.0",
    "lint-staged": "~16.2.3",
    "msw": "~2.11.3",
    "postcss": "~8.5.3",
    "postcss-html": "~1.8.0",
    "postcss-scss": "~4.0.9",
    "prettier": "~3.6.2",
    "rollup-plugin-visualizer": "~6.0.4",
    "sass": "~1.93.2",
    "stylelint": "~16.24.0",
    "stylelint-config-valantic": "~9.0.0",
    "svg-sprite": "~2.0.4",
    "svgo": "~4.0.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "~8.45.0",
    "vite-plugin-compression": "~0.5.1",
    "vite-plugin-image-optimizer": "~2.0.2",
    "vite-plugin-markdown": "~2.2.0",
    "vitest": "~3.2.4",
    "vue-component-type-helpers": "~3.1.0",
    "vue-eslint-parser": "~10.2.0",
    "vue-router": "~4.5.0",
    "vue-tsc": "~3.1.0"
  },
  "browserslist": [
    "> 2%",
    "last 2 years",
    "not ie_mob > 0",
    "not ie > 0",
    "not dead"
  ],
  "msw": {
    "workerDirectory": "public"
  }
}
